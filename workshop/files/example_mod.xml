<Mod xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<!-- 
		JUMPKINGPLUS V1.2.0 by Phoenixx19
		 https://phoenixx19.github.io/JumpKingPlus/workshop/documentation
		 Custom Mod support file for reading (most of) the mod files.
		
		Details:
		 <title> will appear ingame instead of the "Main Babe" text.
		 <ending_screen> is the screen of the babe, do not change this unless you know what you are doing!
		 Inside <Fonts> you can change various JK fonts, idk why you would change them but more freedom I guess.
		 <Ending> is the category for ending images, only one story is available for now.
	-->
	
	<!-- About the mod -->
	<About>
		<title>Sample Title</title>
		<ending_screen>2</ending_screen>
		<disableProgress>true</disableProgress> <!-- optional -->
		<StartPosition> <!-- just dont write this <StartPosition>, if you don't want it -->
			<positionX>240</positionX>
			<positionY>160</positionY>
			<velocityX></velocityX> <!-- optional -->
			<velocityY>10</velocityY> <!-- optional -->
			<isOnGround></isOnGround> <!-- optional -->
		</StartPosition>
	</About>
	
	<!-- Custom fonts, leave blank if default -->
	<Fonts>
		<MenuFont></MenuFont>
		<MenuFontSmall>sf_double_homicide</MenuFontSmall>
		<StyleFont></StyleFont>
		<OptimusUnderline></OptimusUnderline>
		<Tangerine></Tangerine>
		<LocationFont></LocationFont>
		<GargoyleFont></GargoyleFont>
	</Fonts>
	
	<!-- Custom images after ending -->
	<Ending>
		<MainBabe>imagecrown</MainBabe>
		<MainShoes>imageshoes</MainShoes>
	</Ending>
	
	<!-- Ending lines for finishing the mod -->
	<EndingLines>
		<Credit>
			<header>Sample Babe</header>
			<People>
				<string>Author 1</string>
				<string>Author 2</string>
				<string>Author 3</string>
			</People>
		</Credit>
		<Credit>
			<header>language.CreditS_SPECIAL_THANKS_TO</header>
			<People>
				<string>LanguageManager._ERIK</string>
				<string>Phoenixx19</string>
				<string>you! :)</string>
			</People>
		</Credit>
	</EndingLines>
</Mod>